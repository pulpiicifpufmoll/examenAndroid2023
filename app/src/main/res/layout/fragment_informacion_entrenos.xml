<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".ui.fragments.InformacionEntrenos">
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:orientation="vertical">
        <TextView
                android:text="TextView"
                android:textSize="30dp"
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/infoNombre"/>
        <TextView
                android:text="TextView"
                android:padding="3dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/infoEntreno1"/>
        <TextView
                android:text="TextView"
                android:padding="3dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/infoEntreno2"/>
        <TextView
                android:text="TextView"
                android:padding="3dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/infoEntreno3"/>
        <ImageView
                android:id="@+id/imagenEntreno"
                android:src="@drawable/ej1_background"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
        />


    </LinearLayout>
</FrameLayout>